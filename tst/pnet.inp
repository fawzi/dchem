{
startConfig:{
    class:FileConfig,
    fileName:"test.xyz",
    format:"xyz",
    cell:[[8.45,0,0],
        [0,24.0,0],
        [0,0,8.45]]
}

singleP:{
    class:SinglePoint,
    calcE:1,
    calcF:1,
    method:{ class:Ref, name:poorM}
}

poorM:{
    class:PoorManExecuter,
    templateDir:"templates/cp2k1",
    subs:{
        cutoff:"800",
        projName:"test1",
    },
    executeE:"sh calcE.sh",
    executeEF:"sh calcE.sh"
    startConfig:{ class:Ref name:startConfig}
}

MinE:{
    class:MinEExplorer
    nEval:10
}

mullerB:{
    class:MullerBrownPot
    startX:0.45
    startY:0.1
}

cLimiter:{
    class:ContextLimiter,
    maxContexts:1
}

limitedMB:{
    class:ContextLimiterClient,
    contextLimiter:{ class:Ref, name:cLimiter}
    method:{ class:Ref, name:mullerB}
}

workAsker:{
    class:WorkAsker,
    evaluator:{ class:Ref, name:limitedMB}
}

journal:{
    class:PNetJournal
    journalFormat:json
}

initialPos:{
    class:AddRefPos
}

posELog:{
    class:PosELogger,
    baseFileName:"posELog",
    flushEachLine:true
}

spLogger:{
    class:TrackSpecialPoints
    logAllGFlagsChanges:1
}

printAttractors:{
    class:PrintAttractors,
}

pnetSilos:{
    class:PNetSilos,
    evaluator:{ class:Ref, name:limitedMB}
    evaluatorTask:{ class:Ref, name:workAsker}
    explorers:[ { class:Ref, name:MinE },{ class:Ref, name:journal },{ class:Ref, name:posELog } ]
    loaders:[{ class:Ref, name:initialPos },{ class:Ref, name: spLogger }]
    finishers:[{ class:Ref, name: printAttractors }]
}


main:{class:Ref, name:pnetSilos}

}
